           </div>\n        </div><br />\n        <div class=\"container py-5 card rounded\">\n            <h1 v-if=\"!komentars\" class=\"fw-bold\">Belum ada komentar masuk..</h1>\n            <table v-else style=\"width:100%;\">\n                <thead>\n                    <tr>\n                        <th>Nama</th>\n                        <th>Komentar</th>\n                    </tr>\n                </thead>\n           \n                <tbody v-for=\"k in komentars\" v-bind:key=\"k\">\n                    <tr>\n                        <td>{{ k.nama }}</td>\n                        <td>{{ k.komentar }}</td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n</template>\n\n<script>\n    import axios from 'axios'\n    \n    export default {\n        name: 'App',\n        components: {\n            BitcoinCard\n        },\n        data() {\n            return {\n                komentars: false\n            }\n        },\n        methods: {\n            refreshData() {\n                axios.get('https://localhost:5001/komentar/')\n                    .then((response) => {\n                        this.komentars = response.data;\n                    });\n              \n            },\n        },\n        mounted: function () {\n            this.refreshData();\n        },\n      \n    }\n</script>\n\n<style>\n    @import url('https://fonts.googleapis.com/css2?family=Poppins');\n    .curr{\n        border-radius: 35px 0 35px 0;\n    }\n    #app {\n        font-family: Poppins, Avenir, Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 60px;\n    }\n    .rextras{\n        border-radius:100%;\n    }\n</style>\n"]},"metadata":{},"sourceType":"module"}