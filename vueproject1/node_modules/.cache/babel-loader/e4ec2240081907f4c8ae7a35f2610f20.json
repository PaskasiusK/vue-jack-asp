{"ast":null,"code":"import axios from 'axios';\nimport BitcoinCard from '@/components/BitcoinCard.vue';\nimport 'sweetalert2/dist/sweetalert2.min.css';\nimport 'sweetalert2/dist/sweetalert2.min.js';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'App',\n  components: {\n    BitcoinCard\n  },\n\n  data() {\n    return {\n      komentars: false,\n      currency: '',\n      nama: '',\n      komentar: ''\n    };\n  },\n\n  methods: {\n    refreshData() {\n      axios.get('https://localhost:5001/komentar/').then(response => {\n        this.komentars = response.data;\n      });\n    },\n\n    komen() {\n      if (this.nama == '' || this.komentar == '') {\n        Swal.fire('Komentar atau Nama Kosong!', 'Input Kosong', 'warning');\n      } else {\n        axios.post(\"https://localhost:5001/komentar/\", {\n          nama: this.nama,\n          komentar: this.komentar\n        }).then(response => {\n          this.refreshData();\n          Swal.fire(response.data, 'Sukses', 'success');\n        });\n      }\n    }\n\n  },\n  mounted: function () {\n    this.refreshData();\n    axios.get('https://api.coindesk.com/v1/bpi/currentprice.json').then(response => this.currency = response.data.bpi);\n  }\n};","map":{"version":3,"mappings":"AAyCI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAO,sCAAP;AACA,OAAO,qCAAP;AACA,OAAOC,IAAP,MAAiB,aAAjB;AACA,eAAe;EACXC,IAAI,EAAE,KADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;;EAKXI,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,KADR;MAEHC,QAAQ,EAAE,EAFP;MAGHC,IAAI,EAAE,EAHH;MAIHC,QAAQ,EAAE;IAJP,CAAP;EAMH,CAZU;;EAaXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACVX,KAAK,CAACY,GAAN,CAAU,kCAAV,EACKC,IADL,CACWC,QAAD,IAAc;QAChB,KAAKR,SAAL,GAAiBQ,QAAQ,CAACT,IAA1B;MACH,CAHL;IAKH,CAPI;;IAQLU,KAAK,GAAG;MACJ,IAAG,KAAKP,IAAL,IAAa,EAAb,IAAmB,KAAKC,QAAL,IAAiB,EAAvC,EAA0C;QACtCP,IAAI,CAACc,IAAL,CAAU,4BAAV,EAAuC,cAAvC,EAAsD,SAAtD;MACH,CAFD,MAEK;QACDhB,KAAK,CAACiB,IAAN,CAAW,kCAAX,EAA+C;UAC3CT,IAAI,EAAE,KAAKA,IADgC;UAE3CC,QAAQ,EAAE,KAAKA;QAF4B,CAA/C,EAGGI,IAHH,CAGSC,QAAD,IAAc;UAClB,KAAKH,WAAL;UACAT,IAAI,CAACc,IAAL,CAAUF,QAAQ,CAACT,IAAnB,EAAwB,QAAxB,EAAiC,SAAjC;QACH,CAND;MAOJ;IACH;;EApBI,CAbE;EAmCXa,OAAO,EAAE,YAAY;IACjB,KAAKP,WAAL;IACAX,KAAK,CAACY,GAAN,CAAU,mDAAV,EACKC,IADL,CACUC,QAAO,IAAM,KAAKP,QAAL,GAAgBO,QAAQ,CAACT,IAAT,CAAcc,GADrD;EAEH;AAvCU,CAAf","names":["axios","BitcoinCard","Swal","name","components","data","komentars","currency","nama","komentar","methods","refreshData","get","then","response","komen","fire","post","mounted","bpi"],"sourceRoot":"","sources":["C:\\xampp\\htdocs\\Vue Learn\\vueproject1\\vueproject1\\src\\App.vue"],"sourcesContent":["<template>\n    <div class=\"app\">\n        <img alt=\"Vue logo\" src=\"./assets/logo.png\">\n        <h1 class=\"text-white text-bg-success curr container py-3\">Data Inputor</h1>\n        <br />\n        <div class=\"container py-5\">\n            <div class=\"form-floating mb-3\">\n                <input type=\"text\" class=\"form-control\" v-model=\"nama\" id=\"floatingInput\" placeholder=\"Masukkan Nama\">\n                <label for=\"floatingInput\">Nama</label>\n            </div>\n            <div class=\"form-floating\">\n                <input type=\"text\" class=\"form-control\" v-model=\"komentar\" id=\"floatingKomentar\" placeholder=\"Masukkan Komentar\">\n                <label for=\"floatingKomentar\">Komentar</label>\n            </div><br />\n            <div class=\"d-flex justify-content-left\">\n                <a href=\"#\" @click=\"komen\" class=\"btn btn-primary btn-large rextras\"><span class=\"glyphicon glyphicon-send\"></span> Kirim!</a>\n            </div>\n        </div><br />\n        <div class=\"container py-5 card rounded\">\n            <h1 v-if=\"!komentars\" class=\"fw-bold\">Belum ada komentar masuk..</h1>\n            <table v-else style=\"width:100%;\">\n                <thead>\n                    <tr>\n                        <th>Nama</th>\n                        <th>Komentar</th>\n                    </tr>\n                </thead>\n           \n                <tbody v-for=\"k in komentars\" v-bind:key=\"k\">\n                    <tr>\n                        <td>{{ k.nama }}</td>\n                        <td>{{ k.komentar }}</td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n        <BitcoinCard v-bind:currency=\"currency\" />\n    </div>\n</template>\n\n<script>\n    import axios from 'axios'\n    import BitcoinCard from '@/components/BitcoinCard.vue'\n    import 'sweetalert2/dist/sweetalert2.min.css';\n    import 'sweetalert2/dist/sweetalert2.min.js';\n    import Swal from 'sweetalert2';\n    export default {\n        name: 'App',\n        components: {\n            BitcoinCard\n        },\n        data() {\n            return {\n                komentars: false,\n                currency: '',\n                nama: '',\n                komentar: ''\n            }\n        },\n        methods: {\n            refreshData() {\n                axios.get('https://localhost:5001/komentar/')\n                    .then((response) => {\n                        this.komentars = response.data;\n                    });\n              \n            },\n            komen() {\n                if(this.nama == '' || this.komentar == ''){\n                    Swal.fire('Komentar atau Nama Kosong!','Input Kosong','warning');\n                }else{\n                    axios.post(\"https://localhost:5001/komentar/\", {\n                        nama: this.nama,\n                        komentar: this.komentar\n                    }).then((response) => {\n                        this.refreshData();\n                        Swal.fire(response.data,'Sukses','success');\n                    });\n                }\n            },\n        },\n        mounted: function () {\n            this.refreshData();\n            axios.get('https://api.coindesk.com/v1/bpi/currentprice.json')\n                .then(response => (this.currency = response.data.bpi))\n        },\n      \n    }\n</script>\n\n<style>\n    @import url('https://fonts.googleapis.com/css2?family=Poppins');\n    .curr{\n        border-radius: 35px 0 35px 0;\n    }\n    #app {\n        font-family: Poppins, Avenir, Helvetica, Arial, sans-serif;\n        -webkit-font-smoothing: antialiased;\n        -moz-osx-font-smoothing: grayscale;\n        text-align: center;\n        color: #2c3e50;\n        margin-top: 60px;\n    }\n    .rextras{\n        border-radius:100%;\n    }\n</style>\n"]},"metadata":{},"sourceType":"module"}