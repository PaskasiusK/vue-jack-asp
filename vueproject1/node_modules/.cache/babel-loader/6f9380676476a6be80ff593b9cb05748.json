{"ast":null,"code":"import axios from 'axios';\nimport BitcoinCard from '@/components/BitcoinCard.vue';\nimport 'sweetalert2/dist/sweetalert2.min.css';\nimport 'sweetalert2/dist/sweetalert2.min.js';\nimport Swal from 'sweetalert2';\nexport default {\n  name: 'App',\n  components: {\n    BitcoinCard\n  },\n\n  data() {\n    return {\n      komentars: false,\n      currency: '',\n      nama: '',\n      komentar: ''\n    };\n  },\n\n  methods: {\n    refreshData() {\n      axios.get('https://localhost:5001/komentar/').then(response => {\n        this.komentars = response.data;\n      });\n    },\n\n    komen() {\n      if (this.nama == '' || this.komentar == '') {\n        Swal.fire('Komentar atau Nama Kosong!', 'Input Kosong', 'error');\n      } else {\n        axios.post(\"https://localhost:5001/komentar/\", {\n          nama: this.nama,\n          komentar: this.komentar\n        }).then(response => {\n          this.refreshData();\n          Swal.fire(response.data, 'Sukses', 'success');\n        });\n      }\n    }\n\n  },\n  mounted: function () {\n    this.refreshData();\n    axios.get('https://api.coindesk.com/v1/bpi/currentprice.json').then(response => this.currency = response.data.bpi);\n  }\n};","map":{"version":3,"mappings":"AAyCI,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,OAAO,sCAAP;AACA,OAAO,qCAAP;AACA,OAAOC,IAAP,MAAiB,aAAjB;AACA,eAAe;EACXC,IAAI,EAAE,KADK;EAEXC,UAAU,EAAE;IACRH;EADQ,CAFD;;EAKXI,IAAI,GAAG;IACH,OAAO;MACHC,SAAS,EAAE,KADR;MAEHC,QAAQ,EAAE,EAFP;MAGHC,IAAI,EAAE,EAHH;MAIHC,QAAQ,EAAE;IAJP,CAAP;EAMH,CAZU;;EAaXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACVX,KAAK,CAACY,GAAN,CAAU,kCAAV,EACKC,IADL,CACWC,QAAD,IAAc;QAChB,KAAKR,SAAL,GAAiBQ,QAAQ,CAACT,IAA1B;MACH,CAHL;IAKH,CAPI;;IAQLU,KAAK,GAAG;MACJ,IAAG,KAAKP,IAAL,IAAa,EAAb,IAAmB,KAAKC,QAAL,IAAiB,EAAvC,EAA0C;QACtCP,IAAI,CAACc,IAAL,CAAU,4BAAV,EAAuC,cAAvC,EAAsD,OAAtD;MACH,CAFD,MAEK;QACDhB,KAAK,CAACiB,IAAN,CAAW,kCAAX,EAA+C;UAC3CT,IAAI,EAAE,KAAKA,IADgC;UAE3CC,QAAQ,EAAE,KAAKA;QAF4B,CAA/C,EAGGI,IAHH,CAGSC,QAAD,IAAc;UAClB,KAAKH,WAAL;UACAT,IAAI,CAACc,IAAL,CAAUF,QAAQ,CAACT,IAAnB,EAAwB,QAAxB,EAAiC,SAAjC;QACH,CAND;MAOJ;IACH;;EApBI,CAbE;EAmCXa,OAAO,EAAE,YAAY;IACjB,KAAKP,WAAL;IACAX,KAAK,CAACY,GAAN,CAAU,mDAAV,EACKC,IADL,CACUC,QAAO,IAAM,KAAKP,QAAL,GAAgBO,QAAQ,CAACT,IAAT,CAAcc,GADrD;EAEH;AAvCU,CAAf","names":["axios","BitcoinCard","Swal","name","components","data","komentars","currency","nama","komentar","methods","refreshData","get","then","response","komen","fire","post","mounted","bpi"],"sourceRoot":"","sources":["C:\\Kresna\\vue-jack-asp\\vueproject1\\src\\App.vue"],"sourcesContent":["<template>\r\n    <div class=\"app\">\r\n        <img alt=\"Vue logo\" src=\"./assets/logo.png\">\r\n        <h1 class=\"text-white text-bg-success curr container py-3\">Data Inputor</h1>\r\n        <br />\r\n        <div class=\"container py-5\">\r\n            <div class=\"form-floating mb-3\">\r\n                <input type=\"text\" class=\"form-control\" v-model=\"nama\" id=\"floatingInput\" placeholder=\"Masukkan Nama\">\r\n                <label for=\"floatingInput\">Nama</label>\r\n            </div>\r\n            <div class=\"form-floating\">\r\n                <input type=\"text\" class=\"form-control\" v-model=\"komentar\" id=\"floatingKomentar\" placeholder=\"Masukkan Komentar\">\r\n                <label for=\"floatingKomentar\">Komentar</label>\r\n            </div><br />\r\n            <div class=\"d-flex justify-content-left\">\r\n                <a href=\"#\" @click=\"komen\" class=\"btn btn-primary btn-large rextras\"><span class=\"glyphicon glyphicon-send\"></span> Kirim!</a>\r\n            </div>\r\n        </div><br />\r\n        <div class=\"container py-5 card rounded\">\r\n            <h1 v-if=\"!komentars\" class=\"fw-bold\">Belum ada komentar masuk..</h1>\r\n            <table v-else style=\"width:100%;\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>Nama</th>\r\n                        <th>Komentar</th>\r\n                    </tr>\r\n                </thead>\r\n           \r\n                <tbody v-for=\"k in komentars\" v-bind:key=\"k\">\r\n                    <tr>\r\n                        <td>{{ k.nama }}</td>\r\n                        <td>{{ k.komentar }}</td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n        <BitcoinCard v-bind:currency=\"currency\" />\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n    import BitcoinCard from '@/components/BitcoinCard.vue'\r\n    import 'sweetalert2/dist/sweetalert2.min.css';\r\n    import 'sweetalert2/dist/sweetalert2.min.js';\r\n    import Swal from 'sweetalert2';\r\n    export default {\r\n        name: 'App',\r\n        components: {\r\n            BitcoinCard\r\n        },\r\n        data() {\r\n            return {\r\n                komentars: false,\r\n                currency: '',\r\n                nama: '',\r\n                komentar: ''\r\n            }\r\n        },\r\n        methods: {\r\n            refreshData() {\r\n                axios.get('https://localhost:5001/komentar/')\r\n                    .then((response) => {\r\n                        this.komentars = response.data;\r\n                    });\r\n              \r\n            },\r\n            komen() {\r\n                if(this.nama == '' || this.komentar == ''){\r\n                    Swal.fire('Komentar atau Nama Kosong!','Input Kosong','error');\r\n                }else{\r\n                    axios.post(\"https://localhost:5001/komentar/\", {\r\n                        nama: this.nama,\r\n                        komentar: this.komentar\r\n                    }).then((response) => {\r\n                        this.refreshData();\r\n                        Swal.fire(response.data,'Sukses','success');\r\n                    });\r\n                }\r\n            },\r\n        },\r\n        mounted: function () {\r\n            this.refreshData();\r\n            axios.get('https://api.coindesk.com/v1/bpi/currentprice.json')\r\n                .then(response => (this.currency = response.data.bpi))\r\n        },\r\n      \r\n    }\r\n</script>\r\n\r\n<style>\r\n    @import url('https://fonts.googleapis.com/css2?family=Poppins');\r\n    .curr{\r\n        border-radius: 35px 0 35px 0;\r\n    }\r\n    #app {\r\n        font-family: Poppins, Avenir, Helvetica, Arial, sans-serif;\r\n        -webkit-font-smoothing: antialiased;\r\n        -moz-osx-font-smoothing: grayscale;\r\n        text-align: center;\r\n        color: #2c3e50;\r\n        margin-top: 60px;\r\n    }\r\n    .rextras{\r\n        border-radius:100%;\r\n    }\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}